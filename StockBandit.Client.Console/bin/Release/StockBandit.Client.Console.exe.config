<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="ClientBinding" listenBacklog="100" maxConnections="100"
     maxReceivedMessageSize="20000000" />
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://localhost:8000/StockBanditServer"
        binding="netTcpBinding" bindingConfiguration="ClientBinding"
        contract="StockBandit.Model.IBanditService" />
    </client>
  </system.serviceModel>
</configuration>